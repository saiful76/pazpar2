	<set target="134.76.176.48:2021/arxiv" name="pz:xslt" value="SUB-xsl/dc-arxiv.xsl"/>

	<set target="134.76.176.48:2020/jfm" name="catalogueURLHint" value="http://www.emis.de/cgi-bin/jfmen/MATH/JFM/quick.html?first=1&amp;maxdocs=1&amp;type=html&amp;format=complete&amp;an="/>
