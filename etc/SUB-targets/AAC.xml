	<settings src="SUB-settings/gbv-z3950.xml"/>
	<settings>
		<set target="z3950.gbv.de:20012/subgoe_opc" 
			name="pz:name" value="SUB Göttingen"/>
	</settings>


	<settings src="SUB-settings/SUB-Guides.xml"/>
